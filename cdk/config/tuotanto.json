    "scaleReaderWithWriter": true,
    "writerInstanceType": "r6gd.xlarge",
    "readerInstanceType": "r6gd.xlarge",
    "serverlessReader": false,
    "version": {
      "major": "17",
      "full": "17.4"
    }
