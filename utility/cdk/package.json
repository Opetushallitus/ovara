{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "prepare": "cd ../ && husky",
    "lint": "eslint ./bin ./lib --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run lint && tsc",
    "watch": "tsc -w",
    "test": "npm run lint && jest",
    "cdk": "npm run lint && cdk"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@types/jest": "^30.0.0",
    "@types/node": "24.9.2",
    "aws-cdk": "2.1031.1",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.221.1",
    "cdk-nag": "^2.37.55",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  }
}
